<html>
<head>
    <title><%- title || 'ðŸ¦„' %> | <%- h.appName %></title>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDJW4jsPlNKgv6jFm3B5Edp5ywgdqLWdmc&sensor=false"></script>
    <script>
        console.log('Showing Google Maps <%- lat %> | <%- lon %>');
        const myCenter = new google.maps.LatLng('<%- lat %>', '<%- lon %>');

        function initialize() {
            const mapProp = {
                center: myCenter,
                zoom: 14,
                mapTypeId: google.maps.MapTypeId.ROADMAP
            };

            const map = new google.maps.Map(document.getElementById("googleMap"), mapProp);

            const marker = new google.maps.Marker({
                position: myCenter,
            });

            marker.setMap(map);
        }

        google.maps.event.addDomListener(window, 'load', initialize);
    </script>
</head>
<body>
<div id="googleMap" style="width:100%;height:100%;"></div>
</body>
</html>